{
  "name": "markdown-editor",
  "version": "0.0.3",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A markdown editor.",
  "devDependencies": {
    "conventional-github-releaser": "^1.1.1",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp": "^3.8.5",
    "gulp-bump": "^2.1.0",
    "gulp-git": "^1.7.1",
    "run-sequence": "^1.1.5",
    "gulp-util": "^3.0.7"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "markdown-editor.html",
    "markdown-icon-button.html"
  ],
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint markdown-editor.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"markdown-editor:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
}
